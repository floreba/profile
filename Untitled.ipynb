   "source": [
    "def accum(s):\n",
    "    new = []\n",
    "    for idx, x in enumerate(s):\n",
    "        l = x.capitalize()\n",
    "        r = x.lower()\n",
    "        new.append(l+(r*idx))\n",
    "    s = \"-\".join(new)\n",
    "    return s"
   ]
  },
